@Article{Bauer04,
  Title                    = {Novel Superconductivity and Magnetism in CePt3Si},
  Author                   = {Bauer, E.
and Hilscher, G.
and Michor, H.
and Sieberer, M.
and Scheidt, E.W.
and Gribanov, A.
and Seropegin, Yu.
and Rogl, P.
